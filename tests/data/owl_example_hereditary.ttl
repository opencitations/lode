@prefix : <http://example.org/onto#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# ========== ONTOLOGIA ==========

<http://example.org/onto> a owl:Ontology ;
    rdfs:label "Test Ontology - Property Inheritance"@en .

# ========== CLASSI ==========

:Person a owl:Class ;
    rdfs:label "Person"@en .

:Document a owl:Class ;
    rdfs:label "Document"@en .

# ========== PROPRIETÀ MADRE (con domain/range) ==========

:hasRelation a owl:ObjectProperty ;
    rdfs:label "has relation"@en ;
    rdfs:domain :Person ;      # Domain ESPLICITO
    rdfs:range :Document .     # Range ESPLICITO

# ========== PROPRIETÀ FIGLIA (senza domain/range) ==========

:hasCreated a owl:ObjectProperty ;
    rdfs:label "has created"@en ;
    rdfs:subPropertyOf :hasRelation . 